.f64 .flag {
    background-image: url(<%= asset_path 'flags/fixes/flags-64.png' %>);
    background-repeat: no-repeat;
    display: block;
}

.f64 .British_Indian_Ocean_Territory {
    width: 64px;
    height: 64px;
    background-position: -2px -2px;
}

.f64 .Christmas_Islands {
    width: 64px;
    height: 64px;
    background-position: -70px -2px;
}

.f64 .Libya {
    width: 64px;
    height: 64px;
    background-position: -138px -2px;
}
